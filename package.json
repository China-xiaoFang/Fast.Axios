{
  "name": "@fast-china/axios",
  "author": "小方",
  "version": "1.0.12",
  "description": "Fast Axios 请求工具库.",
  "type": "module",
  "keywords": [
    "fast-china",
    "fast",
    "typescript",
    "request",
    "axios",
    "axios-adapter"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://fastdotnet.com",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/China-xiaoFang/fast.axios"
  },
  "bugs": {
    "url": "https://gitee.com/China-xiaoFang/fast.axios/issues"
  },
  "scripts": {
    "check": "vue-tsc",
    "update:version": "tsx scripts/update-version.ts",
    "build": "tsx scripts/build.ts && vue-tsc && vite build --config vite.umd-iife.config.ts && vite build --config vite.es-lib.config.ts && tsup && tsx scripts/move-file.ts",
    "publish": "cd @fast-china/axios && npm publish",
    "lint:eslint": "eslint --max-warnings 0 \"src/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"src/**/*.{vue,js,ts,jsx,tsx,md,json,css,scss,less,html}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier"
  },
  "dependencies": {
    "@types/lodash": "latest",
    "@types/lodash-es": "latest",
    "local-pkg": "^1.1.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "unplugin": "^2.2.0"
  },
  "devDependencies": {
    "@dcloudio/types": "latest",
    "@fast-china/eslint-config": "^1.0.15",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "latest",
    "@vue/tsconfig": "^0.7.0",
    "axios": "^1.8.3",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "terser": "^5.39.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
